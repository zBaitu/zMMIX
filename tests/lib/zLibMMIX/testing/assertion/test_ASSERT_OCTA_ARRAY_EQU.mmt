[TEST_SUITE test_ASSERT_ARRAY_OCTA_EQU]

[TEST_BODY]
    [ASSERT_OCTA_ARRAY_EQU left,right,len]
[TEST_BODY_END]

[TEST_CASE eq_len_0]
left    OCTA #7fffffffffffffff
right   OCTA #7ffeffffffffffff
len     OCTA 0
[TEST_CASE_END]

[TEST_CASE eq_len_1]
left    OCTA #7fffffffffffffff
right   OCTA #7fffffffffffffff
len     OCTA 0
[TEST_CASE_END]

[TEST_CASE eq_part]
left    OCTA 1,2,#7fffffffffffffff
right   OCTA 1,2,#7ffffffffffffffe
len     OCTA 2
[TEST_CASE_END]

[TEST_CASE eq_all]
left    OCTA 1,2,3,#7fffffffffffffff
right   OCTA 1,2,3,#7fffffffffffffff
len     OCTA 4
[TEST_CASE_END]

[TEST_CASE ne_pos_0]
left    OCTA #7fffffffffffffff
right   OCTA #7ffffffffffffffe
len     OCTA 1
[TEST_CASE_END]

[TEST_CASE ne_pos_1]
left    OCTA #7f,#ffffffffffffffff,1
right   OCTA #7f,#fffffffffffffffe,1
len     OCTA 3
[TEST_CASE_END]

[TEST_CASE ne_pos_last]
left    OCTA 1,2,3,4,5,#7fffffffffffffff
right   OCTA 1,2,3,4,5,#7ffffffffffffffe
len     OCTA 6
[TEST_CASE_END]

[TEST_SUITE_END]
