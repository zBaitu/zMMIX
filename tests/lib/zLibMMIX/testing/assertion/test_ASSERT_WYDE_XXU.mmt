%
% EQU
%
[TEST_SUITE test_ASSERT_WYDE_EQU]

[TEST_BODY]
    [ASSERT_WYDE_EQU left,right]
[TEST_BODY_END]

[TEST_CASE equ]
left    WYDE 12345
right   WYDE 12345
[TEST_CASE_END]

[TEST_CASE not_equ]
left    WYDE #ffff
right   WYDE #fffe
[TEST_CASE_END]

[TEST_SUITE_END]


