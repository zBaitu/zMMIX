%
% EQ
%
[TEST_SUITE test_ASSERT_BYTE_EQ]

[TEST_BODY]
    [ASSERT_BYTE_EQ left,right]
[TEST_BODY_END]

[TEST_CASE eq]
left    BYTE 123
right   BYTE 123
[TEST_CASE_END]

[TEST_CASE not_eq]
left    BYTE #80
right   BYTE #81
[TEST_CASE_END]

[TEST_SUITE_END]


