%
% EQU
%
[TEST_SUITE test_ASSERT_OCTA_EQU]

[TEST_BODY]
    [ASSERT_OCTA_EQU left,right]
[TEST_BODY_END]

[TEST_CASE equ]
left    OCTA 12345678987654321
right   OCTA 12345678987654321
[TEST_CASE_END]

[TEST_CASE not_equ]
left    OCTA #ffffffffffffffff
right   OCTA #fffffffffffffffe
[TEST_CASE_END]

[TEST_SUITE_END]


