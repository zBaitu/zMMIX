%
% EQU
%
[TEST_SUITE test_ASSERT_TETRA_EQU]

[TEST_BODY]
    [ASSERT_TETRA_EQU left,right]
[TEST_BODY_END]

[TEST_CASE equ]
left    TETRA 123456789
right   TETRA 123456789
[TEST_CASE_END]

[TEST_CASE not_neu]
left    TETRA #ffffffff
right   TETRA #fffffffe
[TEST_CASE_END]

[TEST_SUITE_END]


