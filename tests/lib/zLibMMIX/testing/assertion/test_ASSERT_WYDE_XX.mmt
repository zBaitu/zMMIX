%
% EQ
%
[TEST_SUITE test_ASSERT_WYDE_EQ]

[TEST_BODY]
    [ASSERT_WYDE_EQ left,right]
[TEST_BODY_END]

[TEST_CASE eq]
left    WYDE 12345
right   WYDE 12345
[TEST_CASE_END]

[TEST_CASE not_eq]
left    WYDE #8000
right   WYDE #8001
[TEST_CASE_END]

[TEST_SUITE_END]


