%
% EQU
%
[TEST_SUITE test_ASSERT_BYTE_EQU]

[TEST_BODY]
    [ASSERT_BYTE_EQU left,right]
[TEST_BODY_END]

[TEST_CASE equ]
left    BYTE 255
right   BYTE 255
[TEST_CASE_END]

[TEST_CASE not_equ]
left    BYTE #ff
right   BYTE #fe
[TEST_CASE_END]

[TEST_SUITE_END]


