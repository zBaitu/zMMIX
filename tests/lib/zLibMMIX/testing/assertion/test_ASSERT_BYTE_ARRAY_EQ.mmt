[TEST_SUITE test_ASSERT_ARRAY_BYTE_EQ]
[TEST_BODY]
    [ASSERT_BYTE_ARRAY_EQ left,right,len]
[TEST_BODY_END]

[TEST_CASE eq_len_0]
left    BYTE 127
right   BYTE 128
len     OCTA 0
[TEST_CASE_END]

[TEST_CASE eq_len_1]
left    BYTE 127
right   BYTE 127
len     OCTA 0
[TEST_CASE_END]

[TEST_CASE eq_part]
left    BYTE 1,2,3
right   BYTE 1,2,4
len     OCTA 2
[TEST_CASE_END]

[TEST_CASE eq_all]
left    BYTE 1,2,3,127
right   BYTE 1,2,3,127
len     OCTA 4
[TEST_CASE_END]

[TEST_CASE ne_pos_0]
left    BYTE #7f
right   BYTE #ff
len     OCTA 1
[TEST_CASE_END]

[TEST_CASE ne_pos_1]
left    BYTE #7f,#ff,1
right   BYTE #7f,#fe,0
len     OCTA 3
[TEST_CASE_END]

[TEST_CASE ne_pos_last]
left    BYTE #1,2,3,4,5,#7f
right   BYTE #1,2,3,4,5,#7e
len     OCTA 6
[TEST_CASE_END]
[TEST_SUITE_END]
