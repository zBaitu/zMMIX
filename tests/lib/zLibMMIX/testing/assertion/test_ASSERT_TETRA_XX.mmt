%
% EQ
%
[TEST_SUITE test_ASSERT_TETRA_EQ]

[TEST_BODY]
    [ASSERT_TETRA_EQ left,right]
[TEST_BODY_END]

[TEST_CASE eq]
left    TETRA 123456789
right   TETRA 123456789
[TEST_CASE_END]

[TEST_CASE not_eq]
left    TETRA #80000000
right   TETRA #80000001
[TEST_CASE_END]

[TEST_SUITE_END]


