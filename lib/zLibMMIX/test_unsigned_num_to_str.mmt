[TEST_SUITE test_UNSIGNED_NUM_TO_STR]
[TEST_BODY]
    LDO     $1,num
    LDA     $2,str
    SET     $3,len
    PUSHJ   $0,:UnsignedNumToStr
    STO     $0,str_begin    
    [EXPECT_STR_EQ  str,expected_str]
    [EXPECT_OCTA_EQ str_begin,expected_str_begin]
[TEST_BODY_END]

[TEST_CASE positive_short_eq]
num                 OCTA    9
str                 BYTE    " ",0
len                 IS      1
expected_str        BYTE    '9',0
str_begin           OCTA
expected_str_begin  OCTA str
[TEST_CASE_END]

[TEST_CASE positive_eq]
num                 OCTA    12321
str                 BYTE    "          ",0
len                 IS      10
expected_str        BYTE    "     12321",0
str_begin           OCTA
expected_str_begin  OCTA str+5
[TEST_CASE_END]

[TEST_CASE positive_long_eq]
num                 OCTA    12345678987654321
str                 BYTE    "                    ",0
len                 IS      20
expected_str        BYTE    "   12345678987654321",0
str_begin           OCTA
expected_str_begin  OCTA str+3
[TEST_CASE_END]

[TEST_CASE negative_short_eq]
num                 OCTA    -1
str                 BYTE    "                    ",0
len                 IS      20
expected_str        BYTE    "18446744073709551615",0
str_begin           OCTA
expected_str_begin  OCTA str
[TEST_CASE_END]

[TEST_CASE positive_byte_max_eq]
num                 OCTA    #ff
str                 BYTE    "   ",0
len                 IS      3
expected_str        BYTE    "255",0
str_begin           OCTA
expected_str_begin  OCTA str
[TEST_CASE_END]

[TEST_CASE positive_wyde_max_eq]
num                 OCTA    #ffff
str                 BYTE    "     ",0
len                 IS      5
expected_str        BYTE    "65535",0
str_begin           OCTA
expected_str_begin  OCTA str
[TEST_CASE_END]

[TEST_CASE positive_tetra_max_eq]
num                 OCTA    #ffffffff
str                 BYTE    "          ",0
len                 IS      10
expected_str        BYTE    "4294967295",0
str_begin           OCTA
expected_str_begin  OCTA str
[TEST_CASE_END]

[TEST_CASE positive_octa_max_eq]
num                 OCTA    #ffffffffffffffff
str                 BYTE    "                    ",0
len                 IS      20
expected_str        BYTE    "18446744073709551615",0
str_begin           OCTA
expected_str_begin  OCTA str
[TEST_CASE_END]
[TEST_SUITE_END]
